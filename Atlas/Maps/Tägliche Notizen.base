formulas:
  Unbenannt: ""
  Monat: Monat.slice(8) + " " + Monat.slice(0, 4)
views:
  - type: cards
    name: Tage als Karten nach KW
    filters:
      and:
        - file.tags.contains("Periodic/Daily")
    groupBy:
      property: KW
      direction: DESC
    order:
      - file.name
      - formula.Monat
    sort:
      - property: Datum
        direction: ASC
    image: note.banner
    cardSize: 130
    imageAspectRatio: 0.55
